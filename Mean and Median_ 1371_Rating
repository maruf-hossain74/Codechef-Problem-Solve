import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        Scanner input = new Scanner(System.in);
        int t = input.nextInt();
        input.nextLine();
        while(t-- > 0) {
            int x = input.nextInt();
            int y = input.nextInt();
            int n = x * 3;
            int a = y, b = y, c = n - (2 * y);
            System.out.println(a + " " + b + " " + c);
        }
	}
}
